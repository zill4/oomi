{
    "family": "oomi",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "<account>.dkr.ecr.<region>.amazonaws.com/oomi-frontend:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        },
        {
            "name": "backend",
            "image": "<account>.dkr.ecr.<region>.amazonaws.com/oomi-backend:latest",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://user:pass@host:5432/oomi"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
}
